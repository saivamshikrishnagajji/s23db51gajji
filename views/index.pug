extends layout

block content
  h1= title
  p Assignment 10 #{title}
  button(type="button", class="btn btn-secondary", onclick="goFaster()") press me
  div#faster Increase the speed
  
  block scripts
    script.
      var mph = 20;
      var change = 1;

      function goFaster() {
        mph += change;
        change += 1;
        document.getElementById("faster").textContent = `MPH is ${mph}  change is ${change}`;
      }

